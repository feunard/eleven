{
	"compilerOptions": {
		"target": "ES2020",
		"module": "CommonJS",
		"strict": true,
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"suppressImplicitAnyIndexErrors": true,
    "outDir": "lib"
  },
  "include": ["src"]
}
